<%= render 'shared/navbar' %>
<div class="search-width">
  <%= form_with url: words_path, method: :get, class: "d-flex" do %>
    <%= text_field_tag :q,
params[:q],
class: "form-control",
placeholder: "Type words to look up"
 %>
    <%= submit_tag "Search", name:"", class: "btn btn-primary" %>
  <% end %>
</div>
